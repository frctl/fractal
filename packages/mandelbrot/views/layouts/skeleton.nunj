<!DOCTYPE html>
<html lang="{{ frctl.web.theme.options.lang | default('en') }}" dir="{{ frctl.web.theme.options.langdir | default('ltr') }}" class="no-js">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon" type="image/x-icon" href="/favicon.ico">

    <script>var cl = document.querySelector('html').classList; cl.remove('no-js'); cl.add('has-js');</script>

    <link rel="stylesheet" type="text/css" href="/{{ frctl.web.theme.options.assetsDir }}/css/{{ frctl.web.theme.options.colorscheme | default('default') }}.css?cachebust={{ frctl.web.theme.version }}">

    <title>{% if pageTitle %}{{ pageTitle }} | {% endif %}{{ frctl.config.project.title }}</title>
</head>
<body>

{% block page %}{% endblock %}

<script src="/{{ frctl.web.theme.options.assetsDir }}/js/build.js"></script>
</body>
</html>
