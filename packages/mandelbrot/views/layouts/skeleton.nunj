<!DOCTYPE html>
<html lang="{{ frctl.theme.config.lang | default('en') }}" dir="{{ 'rtl' if frctl.theme.config.rtl else 'ltr' }}" class="no-js">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon" href="{{ frctl.theme.urlPath('favicon.ico') }}" type="image/ico">
    <link rel="apple-touch-icon" href="{{ frctl.theme.urlPath('/theme/img/mask-icon.svg') }}" type="image/svg">
    <link rel="mask-icon" href="{{ frctl.theme.urlPath('/theme/img/mask-icon.svg') }}" color="{{ frctl.theme.accent }}">

    <script>var cl = document.querySelector('html').classList; cl.remove('no-js'); cl.add('has-js');</script>

    <link rel="stylesheet" href="{{ frctl.theme.urlPath('/theme/css/') }}mandelbrot.css?cachebust={{ frctl.theme.version }}" type="text/css">

    <title>{% if pageTitle %}{{ pageTitle }} | {% endif %}{{ frctl.config.project.title }}</title>
</head>
<body>

{% block page %}{% endblock %}

<script src="{{ frctl.theme.urlPath('/theme/js/mandelbrot.js') }}"></script>
</body>
</html>
