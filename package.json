{
  "name": "@frctl/fractal",
  "version": "2.0.0-alpha.2",
  "description": "A toolkit for parsing and querying filesystem-based component libraries.",
  "homepage": "https://github.com/frctl/fractal",
  "repository": "https://github.com/frctl/fractal",
  "bugs": "https://github.com/frctl/fractal/issues",
  "contributors": [
    "Mark Perkins <mark@allmarkedup.com> (http://allmarkedup.com)",
    "Danielle Huntrods <danielle.huntrods@clearleft.com> (http://clearleft.com)"
  ],
  "bin": {
    "frctl": "./bin/fractal"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 6.0.0"
  },
  "scripts": {
    "test": "npm run lint:fix && nyc npm run test:unit",
    "test:unit": "mocha \"test/**/*.test.js\" --require test/support/env",
    "test:entry": "mocha \"test/factory.test.js\" --require test/support/env",
    "test:watch": "npm run test:unit -- --watch",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "generate:docs": "jsdoc src/*",
    "release": "np",
    "test:plugins": "xo --fix test/plugins/**/*.test.js && xo test/plugins/**/*.js && nyc mocha \"test/plugins/**/*.test.js\" --require test/support/env"
  },
  "dependencies": {
    "@frctl/adapters": "^0.1.0",
    "@frctl/cli": "^0.0.1",
    "@frctl/ffs": "^0.2.0",
    "@frctl/internals": "^0.4.1",
    "@frctl/utils": "^0.5.0",
    "bluebird": "^3.4.7",
    "check-types": "^7.1.5",
    "eventemitter2": "^3.0.0",
    "liftoff": "^2.3.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@frctl/eslint-config-frctl": "0.1.0",
    "@frctl/utils": "^0.5.0",
    "chai": "^3.5.0",
    "jsdoc": "^3.4.3",
    "mocha": "^3.2.0",
    "np": "^2.12.0",
    "nunjucks": "^3.0.0",
    "nyc": "^10.1.2",
    "proxyquire": "^1.7.11",
    "sinon": "^1.17.7",
    "xo": "^0.17.1"
  },
  "xo": {
    "extends": "@frctl/eslint-config-frctl",
    "rules": {
      "no-warning-comments": "off"
    }
  }
}
